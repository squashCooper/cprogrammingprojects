/******************************************************************************
Midterm, Program 4
 Raise a real number a to the real power b

*******************************************************************************/
#include <stdio.h>
#include <iostream>
#include <cmath>
using namespace std;
int power()
{
    int a,b;
    cout << "Enter a positive number ";
    cin>>a;
    cout << "Enter another positive number ";
    cin>>b;
    int raiseAtoB = pow(a,b);
    cout << raiseAtoB;
}
int main()
{
    power();
    return 0;
}
